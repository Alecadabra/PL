# Values -------------------------------------------------------------------- #

# Base name of program
NAME = 

# Source files
SRC = $(NAME).something

# Executable file
EXEC = $(NAME)

# Generated files
GENERATED = $(EXEC)

# Tasks --------------------------------------------------------------------- #

